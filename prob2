#include <iostream>
#include <iomanip>
#include <conio.h>

using namespace std;

int main()
{
	double m, d, v;

	cout << setprecision(2) << showpoint << fixed;
	cout << " Enter mass: " << endl;
	cin >> m;
	cout << " Enter density: " << endl;
	cin >> d;

	v = m / d;

	cout << " Volume is equal to " << v << endl;

	_getch();
	cin.get();
	return 0;
}
